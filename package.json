{
  "name": "eslint-config-modular",
  "version": "7.0.0",
  "description": "Modular ESLint configuration",
  "keywords": [
    "eslintconfig",
    "eslint",
    "check",
    "checker",
    "code",
    "code checker",
    "code linter",
    "code standards",
